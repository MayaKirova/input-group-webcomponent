{
  "name": "input-group-webcomponent",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/input-group-webcomponent/input-group-webcomponent.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.coverage": "stencil test --spec --e2e --coverage",
    "test.azure": "stencil test --spec --e2e --coverage --coverageReporters='cobertura' --reporters='jest-junit' ",
    "generate": "stencil generate",
    "lint": "tslint --project ."
  },
  "devDependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/sass": "^1.1.1",
    "@types/jest": "24.0.25",
    "@types/puppeteer": "1.19.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "1.19.0",
    "tslint-ionic-rules": "0.0.21"
  },
  "license": "MIT",
  "dependencies": {
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0"
  }
}
